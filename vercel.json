{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "functions": {
      "controllers/*.js": {
        "maxDuration": 60
      },
      "middleware/**/*.js": {
        "maxDuration": 60
      }
    }
  }